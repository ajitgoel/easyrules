package com.example.model;

import java.util.List;

public class Person {
    private String focusArea;
    private int isClinicalJourney;
    private String userIntent;
    private List<Zone> zones;
    public Person() {}
    public Person(String focusArea, int isClinicalJourney, String userIntent, List<Zone> zones) {
        this.focusArea = focusArea;
        this.isClinicalJourney = isClinicalJourney;
        this.userIntent = userIntent;
        this.zones = zones;
    }
    public String getFocusArea() {
        return focusArea;
    }
    public void setFocusArea(String focusArea) {
        this.focusArea = focusArea;
    }
    public int getIsClinicalJourney() {
        return isClinicalJourney;
    }
    public void setIsClinicalJourney(int isClinicalJourney) {
        this.isClinicalJourney = isClinicalJourney;
    }
    public String getUserIntent() {
        return userIntent;
    }
    public void setUserIntent(String userIntent) {
        this.userIntent = userIntent;
    }
    public List<Zone> getZones() {
        return zones;
    }
    public void setZones(List<Zone> zones) {
        this.zones = zones;
    }
}
